@model IEnumerable<StudioSintoniaPreview.Models.LoginModel>

@{
    ViewData["Title"] = "Index";
}
<head>
    <link rel="stylesheet" href="~/css/Styles.css" />
</head>
<br />

<h1>Usuarios</h1>

<p>
    <a asp-action="Create">Criar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Login)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Senha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Login)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Senha)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LoginModelId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LoginModelId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LoginModelId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
